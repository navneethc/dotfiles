# Python aliases
alias venvup='source venv/bin/activate'

# Poetry
alias posh='poetry shell'

# Git aliases
alias ga='git add'
alias gb='git checkout -b'
alias gch='git checkout'
alias gco='git commit'
alias gl='git log'
alias glo='git log --oneline'
alias glp='git log --pretty=oneline'
alias gp='git push'
alias gs='git status'

# Postgres aliases
alias pgup='pg_ctlcluster 12 main start'
alias pgdown='pg_ctlcluster 12 main stop'
